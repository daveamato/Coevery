<div class="btn-toolbar">
    <button class="btn btn-small btn-success" data-ng-click="add()"
        data-ng-disabled="isPublished=='False'" title='@Model["PublishTip"]'>
        <i class="icon-plus"></i>&nbsp;Add
    </button>
    <button class="btn btn-small" data-ng-click="getAll()" 
        data-ng-disabled="isPublished=='False'" title='@Model["PublishTip"]'>
        <i class="icon-refresh"></i>&nbsp;Refresh
    </button>
    <div class="btn-group" ng-show="selectedItems.length > 0">
        <button class="btn btn-small" data-ng-click="edit(selectedItems[0])" ng-disabled="selectedItems.length > 1">
            <i class="icon-edit"></i>&nbsp;Edit
        </button>
        <button class="btn btn-small" co-delete-button confirm-message="You really want to delete this view?" delete-action="delete()">
            <i class="icon-remove"></i>&nbsp;Delete
        </button>
        <button class="btn btn-small" data-ng-click="setDefault(selectedItems[0])" ng-disabled="selectedItems.length > 1">
            Default
        </button>
    </div>
</div>

<div class="row-fluid" data-ag-grid="gridOptions" data-ag-grid-name="viewList"> </div>

