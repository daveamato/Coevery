<section data-ng-init="getAllRelationship()">
    <div class="btn-toolbar">
        <div class="btn-group">
            <button data-ng-disabled="isPublished=='False'" title='@Model["PublishTip"]'
                class="btn btn-small btn-success dropdown-toggle" data-toggle="dropdown">
                <i class="icon-plus"></i>&nbsp;Add <span class="caret"></span>
            </button>
            <ul class="dropdown-menu">
                <li><a ng-click="createOneToMany()"><i class="icon-plus"></i>One-to-Many Relationship</a></li>
                <li><a ng-click="createManyToMany()"><i class="icon-plus"></i>Many-to-Many Relationship</a></li>
            </ul>
        </div>
    </div>
    <div class="row-fluid" data-ag-grid="gridOptions" data-ag-grid-name="relationList"> </div>
</section>
